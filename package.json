{
  "name": "combine-loader",
  "version": "0.5.0",
  "description": "webpack loader to combine results from multiple loaders into one object",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src --out-dir lib",
    "clean": "del-cli ./example/build/ ./lib/",
    "develop": "babel src --watch --out-dir lib",
    "jest": "jest",
    "lint": "eslint .",
    "prepublish": "yarn run build",
    "test": "yarn run clean && yarn run build && yarn run lint && yarn run jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elliottsj/combine-loader.git"
  },
  "keywords": [
    "combine",
    "loader",
    "webpack"
  ],
  "author": "Spencer Elliott <me@elliottsj.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/elliottsj/combine-loader/issues"
  },
  "homepage": "https://github.com/elliottsj/combine-loader#readme",
  "dependencies": {
    "loader-utils": "^1.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "del-cli": "^1.1.0",
    "eslint": "^5.10.0",
    "eslint-plugin-prettier": "^3.0.0",
    "front-matter-loader": "^0.2.0",
    "highlight.js": "^9.13.1",
    "html-loader": "^0.5.5",
    "jest": "^23.6.0",
    "json-loader": "^0.5.7",
    "markdown-it": "^8.4.2",
    "markdown-it-loader": "^0.7.0",
    "prettier": "^1.15.3",
    "raw-loader": "^0.5.1",
    "webpack": "^4.27.1"
  },
  "jest": {
    "roots": [
      "<rootDir>/example",
      "<rootDir>/src"
    ],
    "testEnvironment": "node"
  }
}
