{
  "name": "combine-loader",
  "version": "0.3.1",
  "description": "webpack loader to combine results from multiple loaders into one object",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src --out-dir lib",
    "clean": "del-cli ./example/build/ ./lib/",
    "develop": "babel src --watch --out-dir lib",
    "jest": "jest",
    "lint": "eslint .",
    "prepublish": "npm run build",
    "test": "npm run clean && npm run build && npm run lint && npm run jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elliottsj/combine-loader.git"
  },
  "keywords": [
    "combine",
    "loader",
    "webpack"
  ],
  "author": "Spencer Elliott <me@elliottsj.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/elliottsj/combine-loader/issues"
  },
  "homepage": "https://github.com/elliottsj/combine-loader#readme",
  "dependencies": {
    "loader-utils": "^1.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-jest": "^22.4.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-1": "^6.3.13",
    "del-cli": "^1.1.0",
    "eslint": "^4.2.0",
    "eslint-plugin-prettier": "^2.1.2",
    "front-matter-loader": "^0.2.0",
    "html-loader": "^0.5.0",
    "highlight.js": "^9.11.0",
    "jest": "^22.4.2",
    "json-loader": "^0.5.4",
    "markdown-it": "^8.3.1",
    "markdown-it-loader": "elliottsj/markdown-it-loader#loader-options",
    "prettier": "^1.5.3",
    "raw-loader": "^0.5.1",
    "webpack": "^4.1.1"
  },
  "jest": {
    "roots": [
      "<rootDir>/example",
      "<rootDir>/src"
    ],
    "testEnvironment": "node"
  }
}
