{
  "name": "combine-loader",
  "version": "0.2.0",
  "description": "webpack loader to combine results from multiple loaders into one object",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src --out-dir lib",
    "develop": "babel src --watch --out-dir lib",
    "jest": "jest",
    "lint": "eslint .",
    "prepublish": "npm run build",
    "test": "npm run lint && npm run jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elliottsj/combine-loader.git"
  },
  "keywords": [
    "combine",
    "loader",
    "webpack"
  ],
  "author": "Spencer Elliott <me@elliottsj.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/elliottsj/combine-loader/issues"
  },
  "homepage": "https://github.com/elliottsj/combine-loader#readme",
  "dependencies": {
    "loader-utils": "^0.2.11"
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-eslint": "^7.0.0",
    "babel-jest": "^16.0.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-1": "^6.3.13",
    "eslint": "^3.3.1",
    "eslint-config-airbnb-base": "^7.0.1",
    "eslint-plugin-import": "^2.1.0",
    "front-matter-loader": "^0.1.2",
    "html-loader": "^0.4.3",
    "jest-cli": "^19.0.2",
    "json-loader": "^0.5.4",
    "markdown-it-loader": "^0.3.2",
    "raw-loader": "^0.5.1",
    "webpack": "^2.1.0-beta.2"
  },
  "jest": {
    "testPathDirs": [
