{
  "parser": "babel-eslint",
  "extends": "airbnb-base",
  "env": {
    "jasmine": true,
    "jest": true
  },
  "rules": {
    "arrow-parens": "off",
    "func-names": "off",
    "import/imports-first": "off",
    "import/no-extraneous-dependencies": ["error", {
      "devDependencies": true
    }]
  }
}
